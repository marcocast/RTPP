<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rtpp.rtpp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SigninActivity"
            android:label="@string/title_activity_signin"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".SignupActivity"
            android:label="@string/title_activity_signup"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".JoinStartActivity"
            android:label="@string/title_activity_join_start"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".JoinActivity"
            android:label="@string/title_activity_join"
            android:parentActivityName=".JoinStartActivity" >
        </activity>
        <activity
            android:name=".EstimationActivity"
            android:label="@string/title_activity_estimation"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".CardActivity"
            android:label="@string/title_activity_card"
            android:parentActivityName=".EstimationActivity" >
        </activity>
        <activity
            android:name=".CreateActivity"
            android:label="@string/title_activity_create"
            android:parentActivityName=".JoinStartActivity" >
        </activity>
        <activity
            android:name=".UserProfileActivity"
            android:label="@string/title_activity_user_profile"
            android:parentActivityName=".JoinStartActivity" >
        </activity>
    </application>

</manifest>
